msgstr "      --port TCP-порт для під’єднання (застарів, використовуйте --service)\n"

#: options.c:664
msgid "  -P, --service     TCP service to connect to (can be numeric TCP port)\n"
msgstr "  -P, --service служба TCP для під’єднання (може бути числовим TCP-портом)\n"

#: options.c:665
msgid "      --auth        authentication type (password/kerberos/ssh/otp)\n"
msgstr "      --auth тип автентифікації (password/kerberos/ssh/otp)\n"

#: options.c:666
msgid "  -t, --timeout     server nonresponse timeout\n"
msgstr "  -t, --timeout таймаут мовчання сервера\n"

#: options.c:667
msgid "  -E, --envelope    envelope address header\n"
msgstr "  -E, --envelope заголовок охоплюючої адреси\n"

#: options.c:668
