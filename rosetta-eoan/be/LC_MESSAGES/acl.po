msgstr "%s: Выдаліць у пачатку '/' з абсалютных назваў шляхаў\n"

#: tools/getfacl.c:568
#, c-format
msgid "%s %s -- get file access control lists\n"
msgstr "%s %s -- атрымлівае файлавыя спісы кантролю доступу\n"

#: tools/getfacl.c:570 tools/getfacl.c:750
#, c-format
msgid "Usage: %s [-%s] file ...\n"
msgstr "Выкарыстанне: %s [-%s] файла ...\n"

#: tools/getfacl.c:576
#, c-format
msgid "  -d, --default           display the default access control list\n"
msgstr "  -d, --default           адлюстроўвае спісы кантролю доступу па змаўчанні\n"

#: tools/getfacl.c:580
#, c-format
msgid "  -a,  --access           display the file access control list only\n"
"  -d, --default           display the default access control list only\n"
"  -c, --omit-header       do not display the comment header\n"
"  -e, --all-effective     print all effective rights\n"
"  -E, --no-effective      print no effective rights\n"
"  -s, --skip-base         skip files that only have the base entries\n"
"  -R, --recursive         recurse into subdirectories\n"
"  -L, --logical           logical walk, follow symbolic links\n"
"  -P, --physical          physical walk, do not follow symbolic links\n"
"  -t, --tabular           use tabular output format\n"
"  -n, --numeric           print numeric user/group identifiers\n"
"  -p, --absolute-names    don't strip leading '/' in pathnames\n"
msgstr "  -a,  --access           адлюстраваць толькі спіс кантролю доступу да файла\n"
"  -d, --default           адлюстраваць толькі спіс кантролю доступу па змаўчанні\n"
"  -c, --omit-header       не адлюстроўваць загаловак з каментарыямі\n"
"  -e, --all-effective     надрукаваць усе дзеючыя правы\n"
"  -E, --no-effective      надрукаваць правы, якія не дзейнічаюць\n"
"  -s, --skip-base         прапускаць файлы, якія маюць толькі базавыя запіс\n"
"  -R, --recursive         рэкурсія ў падкаталогах\n"
"  -L, --logical            лагічны праход, прытрымлівацца сімвалічных спасылак\n"
"  -P, --physical          фізічны праход, не прытрымлівацца сімвалічных спасылак\n"
"  -t, --tabular           выкарыстоўваць фармат вываду з табуляцыяй\n"
"  -n, --numeric           друкаваць лічбавыя ідэнтыфікатары карыстальніка/групы\n"
"  -p, --absolute-names    не прыбіраць '/' у пачатку шляху каталогаў\n"

#: tools/getfacl.c:595 tools/setfacl.c:299
#, c-format
msgid "  -v, --version           print version and exit\n"
"  -h, --help              this help text\n"
msgstr "  -v, --version           надрукаваць версію і выйсці\n"
"  -h, --help              тэкст даведкі\n"

#: tools/getfacl.c:737 tools/setfacl.c:316
#, c-format
msgid "%s: Standard input: %s\n"
msgstr "%s: стандартны ўвод: %s\n"

#: tools/getfacl.c:752 tools/setfacl.c:655
#, c-format
msgid "Try `%s --help' for more information.\n"
msgstr "Паспрабуйце \"%s --help\", каб атрымаць больш звестак.\n"

#: tools/do_set.c:410
#, c-format
msgid "%s: %s: Malformed access ACL `%s': %s at entry %d\n"
msgstr "%s: %s: кепска складзены спіс доступу \"%s\": %s побач з пунктам %d\n"

#: tools/do_set.c:437
#, c-format
msgid "%s: %s: Malformed default ACL `%s': %s at entry %d\n"
msgstr "%s: %s: няправільны ACL па змаўчанні `%s': %s у запісе %d\n"

#: tools/do_set.c:508
#, c-format
msgid "%s: %s: Only directories can have default ACLs\n"
msgstr "%s: %s: толькі каталогі могуць мець ACL па змаўчанні\n"

#: tools/setfacl.c:147
#, c-format
msgid "%s: %s: No filename found in line %d, aborting\n"
msgstr "%s: %s: не знойдзена назва файла ў радку %d; аперацыя скасаваная\n"

#: tools/setfacl.c:152
#, c-format
msgid "%s: No filename found in line %d of standard input, aborting\n"
msgstr "%s: не знойдзена назва файла ў радку %d стандартнага ўводу; аперацыя скасаваная\n"

#: tools/setfacl.c:173
#, c-format
msgid "%s: %s: %s in line %d\n"
msgstr "%s: %s: %s у радку %d\n"

#: tools/setfacl.c:205
#, c-format
msgid "%s: %s: Cannot change owner/group: %s\n"
msgstr "%s: %s: немагчыма змяніць уладальніка/групу: %s\n"

#: tools/setfacl.c:224
#, c-format
msgid "%s: %s: Cannot change mode: %s\n"
msgstr "%s: %s: немагчыма змяніць рэжым: %s\n"

#: tools/setfacl.c:266
#, c-format
msgid "%s %s -- set file access control lists\n"
msgstr "%s %s -- устанаўлівае файлавыя спісы кантролю доступу\n"

#: tools/setfacl.c:268 tools/setfacl.c:653
#, c-format
msgid "Usage: %s %s\n"
msgstr "Выкарыстанне: %s %s\n"

#: tools/setfacl.c:271
#, c-format
msgid "  -m, --modify=acl        modify the current ACL(s) of file(s)\n"
"  -M, --modify-file=file  read ACL entries to modify from file\n"
"  -x, --remove=acl        remove entries from the ACL(s) of file(s)\n"
"  -X, --remove-file=file  read ACL entries to remove from file\n"
"  -b, --remove-all        remove all extended ACL entries\n"
"  -k, --remove-default    remove the default ACL\n"
msgstr "  -m, --modify=acl        змяніць бягучы ACL файла/файлаў\n"
"  -M, --modify-file=file  прачытаць запісы ACL, каб змяніць іх у файле\n"
"  -x, --remove=acl        выдаліць запісы з ACL файла/файлаў\n"
"  -X, --remove-file=file  прачытаць запісы ACL, каб выдаліць іх з файла\n"
"  -b, --remove-all        выдаліць усе пашыраныя запісы ACL\n"
"  -k, --remove-default    выдаліць ACL па змаўчанні\n"

#: tools/setfacl.c:280
#, c-format
msgid "      --set=acl           set the ACL of file(s), replacing the current ACL\n"
"      --set-file=file     read ACL entries to set from file\n"
"      --mask              do recalculate the effective rights mask\n"
msgstr "      --set=acl           устанавіць  ACL файла/файлаў, шляхам замены бягучага ACL\n"
"      --set-file=file     прачытаць запісы ACL, каб устанавіць іх з файла\n"
"      --mask              паўторна зрабіць падлік маскі эфектыўных правоў\n"

#: tools/setfacl.c:286
#, c-format
msgid "  -n, --no-mask           don't recalculate the effective rights mask\n"
"  -d, --default           operations apply to the default ACL\n"
msgstr "  -n, --no-mask          не рабіць паўторны падлік маскі эфектыўных правоў\n"
"  -d, --default           дзеянні ўжываюцца да ACL па змаўчанні\n"

#: tools/setfacl.c:291
#, c-format
msgid "  -R, --recursive         recurse into subdirectories\n"
"  -L, --logical           logical walk, follow symbolic links\n"
"  -P, --physical          physical walk, do not follow symbolic links\n"
"      --restore=file      restore ACLs (inverse of `getfacl -R')\n"
"      --test              test mode (ACLs are not modified)\n"
msgstr "  -R, --recursive         рэкурсіўна ў падкаталогу\n"
"  -L, --logical          лагічны праход, прытрымлівацца сімвалічных спасылак\n"
"  -P, --physical          фізічны праход, не прытрымлівацца сімвалічных спасылак\n"
"      --restore=file      аднавіць ACL (зваротнае дзеянне каманды `getfacl -R')\n"
"      --test              тэставы рэжым (ACL не будзе зменены)\n"

