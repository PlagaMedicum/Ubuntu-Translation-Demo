#, c-format
msgid "%s: Read error: %s"
msgstr "%s: Ошибка чтения: %s"

#: src/xz/file_io.c:1177
#, c-format
msgid "%s: Error seeking the file: %s"
msgstr "%s: Ошибка позиционирования в файле: %s"

