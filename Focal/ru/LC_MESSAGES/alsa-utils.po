msgstr " * Подключение/отключение между двумя портами\n"

#: seq/aconnect/aconnect.c:53
#, c-format
msgid "   aconnect [-options] sender receiver\n"
msgstr "   aconnect [-опции] отправитель получатель\n"

#: seq/aconnect/aconnect.c:54
#, c-format
