msgid "Usage:\n"
msgstr "Використання:\n"

#: /opt/gnome/source/glib/gio/gapplication-tool.c:114
#: /opt/gnome/source/glib/gio/gresource-tool.c:551
#: /opt/gnome/source/glib/gio/gsettings-tool.c:696
msgid "Arguments:\n"
msgstr "Аргументи:\n"

