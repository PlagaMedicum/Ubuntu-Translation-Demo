msgid "FILE"
msgstr "ФАЙЛ"

#: /opt/gnome/source/glib/gio/gapplication-tool.c:72
#| msgid "Optional relative or relative filenames, or URIs to open"
msgid "Optional relative or absolute filenames, or URIs to open"
