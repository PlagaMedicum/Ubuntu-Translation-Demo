#: ../gcab.c:62
#, c-format
msgid "Removing leading '%s' from member names"
msgstr "Видалення початкового \"%s\" з імен учасників"

#: ../gcab.c:74
#, c-format
msgid "Dumping %s data to: %s ...\n"
msgstr "Створення дампу даних %s на: %s …\n"

#: ../gcab.c:82
#, c-format
msgid "can't write file %s: %s"
msgstr "неможливо записати файл %s: %s"

#: ../gcab.c:109
msgid "Print program version"
msgstr "Додати версію програми"

#: ../gcab.c:110
msgid "Be verbose"
msgstr "Детально"

#: ../gcab.c:111
msgid "Create archive"
msgstr "Створити архів"

#: ../gcab.c:112
msgid "Extract all files"
msgstr "Розпакувати всі файли"

#: ../gcab.c:113
msgid "Dump reserved and extra data"
msgstr "Додавати в дамп резервні та додаткові дані"

#: ../gcab.c:114
msgid "List content"
msgstr "Показати вміст"

#: ../gcab.c:115
msgid "List content with file details"
msgstr "Показати вміст та відомості про файли"

#: ../gcab.c:116
msgid "Change to directory DIR"
msgstr "Змінити на каталог КАТАЛОГ"

#: ../gcab.c:116
msgid "DIR"
msgstr "КАТАЛОГ"

#: ../gcab.c:117
msgid "Use zip compression"
msgstr "Використовувати zip-стиснення"

#: ../gcab.c:118
msgid "Do not include path"
msgstr "Не додавати шлях"

#: ../gcab.c:119
msgid "Reserve space in cabinet for signing (e.g. -s 6144 reserves 6K bytes)"
msgstr ""
"Зарезервувати місце в архіві для підпису (наприклад, -s 6144 зарезервує 6 Кб)"

#: ../gcab.c:120
msgid "FILE INPUT_FILES..."
msgstr "ФАЙЛ ВХІДНІ_ФАЙЛИ..."

#: ../gcab.c:134
msgid "- create a Cabinet file"
msgstr "- створити Cab-файл"

#: ../gcab.c:135
#, c-format
msgid "Report bugs to <%s>"
msgstr "Надіслати звіт про помилку на <%s>"

#: ../gcab.c:138
msgid ""
"gcab saves many files together into a cabinet archive, and can restore\n"
"individual files from the archive."
msgstr ""
"gcab зберігає в cab-архів декілька файлів та може розпаковувати\n"
"з архіву окремі файли."

#: ../gcab.c:145
#, c-format
msgid "option parsing failed: %s\n"
msgstr "не вдалося розібрати параметри: %s\n"

#: ../gcab.c:154
msgid "Please specify a single operation."
msgstr "Оберіть одну операцію."

#: ../gcab.c:157
msgid "cabinet file must be specified."
msgstr "має бути вказаний файл архіву."

#: ../gcab.c:171
#, c-format
msgid "can't open %s for reading: %s\n"
msgstr "не вдалося відкрити  %s для читання: %s\n"

#: ../gcab.c:173
#, c-format
msgid "error reading %s: %s\n"
msgstr "помилка читання %s: %s\n"

#: ../gcab.c:208
#, c-format
msgid "error during extraction: %s"
msgstr "помилка під час розпакування: %s"

#: ../gcab.c:220
#, c-format
msgid "error while reading signature: %s"
msgstr "помилка читання підпису: %s"

#: ../gcab.c:231
msgid "please specify input files."
msgstr "будь ласка, вкажіть вхідні файли."

#: ../gcab.c:249
#, c-format
msgid "Can't add file %s: %s"
msgstr "Неможливо додати файл %s: %s"

#: ../gcab.c:259
msgid "no files to be archived."
msgstr "файли для архівування відсутні."

#: ../gcab.c:265
#, c-format
msgid "can't create cab file %s: %s"
msgstr "неможливо створити cab-файл %s: %s"

#: ../gcab.c:269
#, c-format
msgid "can't add folder to cab file %s: %s"
msgstr "неможливо додати теку в cab-файл %s: %s"

#: ../gcab.c:277
#, c-format
msgid "can't write cab file %s: %s"
msgstr "неможливо записати cab-файл %s: %s"

#: ../libgcab/cabinet.c:20 ../libgcab/cabinet.c:479
#, c-format
msgid "unsupported compression method %d"
msgstr "непідтримуваний метод стиснення %d"

#: ../libgcab/cabinet.c:499
msgid "tried to decompress %"
msgstr "спроб розпакувати %"

#: ../libgcab/cabinet.c:502
msgid "incorrect checksum detected"
msgstr "виявлена неправильна контрольна сума"
